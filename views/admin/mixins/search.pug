mixin search(keyWord)
    form(id="form-search")
        .input-group 
            input(
                type="text", 
                placeholder="Input here!"
                name="keyword"
                class="form-control"
                value=keyWord
            )
            .input-group-append     
                button(
                    class="btn btn-success"
                    type="submit"
                ) Search